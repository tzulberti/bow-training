<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ar.com.tzulberti.bowtraining.DisplayMessageActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textTotalRepetitionsValue"
        android:layout_width="77dp"
        android:layout_height="30dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textCurrentTime"
        android:layout_marginStart="6dp"
        android:layout_marginTop="72dp"
        app:layout_constraintTop_toBottomOf="@+id/textTotalSeriesValue"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="71dp"
        app:layout_constraintLeft_toRightOf="@+id/textView13"
        android:layout_marginLeft="6dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:layout_marginStart="1dp"
        android:text="@string/current_repetition"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textCurrentRepetitionValue"
        app:layout_constraintBaseline_toBaselineOf="@+id/textCurrentRepetitionValue"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView2"
        android:layout_marginLeft="1dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="123dp"
        android:layout_height="26dp"
        android:text="@string/total_serie"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="3dp"
        app:layout_constraintRight_toRightOf="@+id/textView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textTotalSeriesValue"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="3dp" />

    <TextView
        android:id="@+id/textTotalSeriesValue"
        android:layout_width="0dp"
        android:layout_height="26dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textTotalRepetitionsValue"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/textCurrentSerieValue"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:text="@string/current_serie"
        app:layout_constraintRight_toLeftOf="@+id/textCurrentRepetitionValue"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/textCurrentSerieValue"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView13" />

    <Button
        android:id="@+id/button2"
        android:layout_width="248dp"
        android:layout_height="48dp"
        android:onClick="stop"
        android:text="@string/button_stop"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="49dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="49dp" />

    <TextView
        android:id="@+id/textCurrentSerieValue"
        android:layout_width="0dp"
        android:layout_height="26dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintRight_toRightOf="@+id/textTotalSeriesValue"
        android:layout_marginTop="88dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="90dp"
        app:layout_constraintLeft_toLeftOf="@+id/textTotalSeriesValue"
        app:layout_constraintTop_toTopOf="@+id/button2" />

    <TextView
        android:id="@+id/textCurrentRepetitionValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView2"
        android:layout_marginTop="215dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView2" />

    <TextView
        android:id="@+id/textCurrentRepetitionValue"
        android:layout_width="77dp"
        android:layout_height="30dp"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView8"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="@string/repeticiones_totales"
        android:layout_marginStart="32dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textTotalRepetitionsValue"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="32dp" />

    <TextView
        android:id="@+id/textCurrentTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="132dp"
        app:layout_constraintTop_toBottomOf="@+id/textCurrentRepetitionValue"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="130dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="56dp" />

</android.support.constraint.ConstraintLayout>
